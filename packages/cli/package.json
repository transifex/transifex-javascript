{
  "name": "@transifex/cli",
  "description": "i18n CLI tool for Transifex Native",
  "version": "4.2.3",
  "author": "Transifex",
  "keywords": [
    "transifex",
    "i18n",
    "l10n",
    "localization",
    "internationalization",
    "globalization",
    "translation",
    "cli"
  ],
  "homepage": "https://github.com/transifex/transifex-javascript/tree/master/packages/cli",
  "repository": "git://github.com/transifex/transifex-javascript.git",
  "bugs": "https://github.com/transifex/transifex-javascript/issues",
  "license": "Apache-2.0",
  "bin": {
    "txjs-cli": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/parser": "^7.18.9",
    "@babel/traverse": "^7.18.9",
    "@colors/colors": "^1.5.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^5.1.12",
    "@transifex/native": "^4.2.3",
    "angular-html-parser": "^1.8.0",
    "axios": "^0.27.2",
    "cli-ux": "^6.0.9",
    "ejs": "^3.1.8",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "pug": "^3.0.2",
    "shelljs": "^0.8.5",
    "vue-template-compiler": "^2.7.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^2.1.0",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.26.0",
    "globby": "^11.1.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.8",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "txjs-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest",
    "lint": "eslint src/ test/",
    "test": "nyc mocha \"test/**/*.test.js\"",
    "publish-npm": "npm publish"
  }
}
